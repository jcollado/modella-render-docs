{
  "name": "modella-render-docs",
  "version": "0.3.1",
  "description": "Render documentation from a modella model",
  "main": "index.js",
  "scripts": {
    "cover": "istanbul cover _mocha",
    "coveralls": "istanbul cover _mocha --report lcovonly && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "doc": "jsdoc .",
    "lint": "jshint *.js",
    "postversion": "git push && git push --tags",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "test": "mocha",
    "test:watch": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcollado/modella-render-docs"
  },
  "keywords": [
    "modella",
    "render",
    "documentation"
  ],
  "author": "Javier Collado <javier.collado@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.3.0",
    "coveralls": "^2.11.4",
    "ghooks": "^0.3.2",
    "istanbul": "^0.3.20",
    "jsdoc": "^3.3.3",
    "jshint": "^2.8.0",
    "mocha": "^2.3.3",
    "modella": "^0.2.14",
    "sinon": "^1.17.0",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "ramda": "^0.17.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test"
    }
  }
}
